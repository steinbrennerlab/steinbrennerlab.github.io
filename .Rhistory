mutate_cond <- function(.data, condition, ..., envir = parent.frame())
{
condition <- eval(substitute(condition), .data, envir)
condition[is.na(condition)] = FALSE
.data[condition, ] <- .data[condition, ] %>% mutate(...)
.data
}
